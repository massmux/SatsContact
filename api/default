server {
        listen 443 ssl;
        server_name api.sats.contact;

        access_log /var/log/nginx/reverse-access.log; 
        error_log /var/log/nginx/reverse-error.log;

        location / {
            include uwsgi_params;
            uwsgi_pass unix:/tmp/uwsgi.socket;
        }

        ssl_certificate /etc/letsencrypt/live/api.sats.contact/fullchain.pem; # Point to the fullchain.pem
        ssl_certificate_key /etc/letsencrypt/live/api.sats.contact/privkey.pem; # Point to the private key.

}


